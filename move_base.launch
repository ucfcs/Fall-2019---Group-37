<launch>

   <master auto="start"/>
 <!-- Run the map server --> 
    <node name="map_server" pkg="hector_map_server" type="hector_map_server" args=""/>

   <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
    <rosparam file="$(find egoat)/costmap_common_params.yaml" command="load" ns="global_costmap" /> 
    <rosparam file="$(find egoat)/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find egoat)/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find egoat)/global_costmap_params.yaml" command="load" /> 
    <rosparam file="$(find egoat)/base_local_planner_params.yaml" command="load" />
 </node>

</launch>